    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "geopattern": "^1.2.3",
    "jwks-rsa": "^1.8.0",
    "mongoose": "^5.9.13",
    "multer": "^1.4.2",
    "nodemon": "^2.0.3",
